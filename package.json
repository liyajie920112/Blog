{
  "name": "blog-new",
  "version": "1.0.0",
  "description": "My ultimate Nuxt.js project",
  "author": "liyajie",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
